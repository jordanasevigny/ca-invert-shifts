function swapDivClassname(pLink, pId, pClassName){
	var lDiv = document.getElementById(pId);
	if( lDiv ){
		if( lDiv.className == pClassName ){
			lDiv.className = '';
			pLink.className = 'inactive'
		}else{
			lDiv.className = pClassName;
			pLink.className = ''
		}
	}
}
